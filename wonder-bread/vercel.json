{
  "version": 2,
  "framework": "react",
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "JWT_SECRET_KEY": "@wonder_bread_jwt_secret"
  }
}
